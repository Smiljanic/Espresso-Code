myconfig-final.hpp: FORCE
	@config_files="\
	  $(top_builddir)/myconfig.hpp \
	  $(top_srcdir)/myconfig.hpp \
	  $(srcdir)/myconfig-default.hpp"; \
	for file in $$config_files; do \
	  if test -e $$file; then \
	    echo "  GEN      $@ <= $$file"; \
	    myconfig_found=1; \
	    if test -e $@; then \
	      cmp -s $$file $@ || cp $$file $@; \
	    else \
	       cp $$file $@; \
	    fi; \
	    break; \
	  fi; \
	done; \
	if test ! $$myconfig_found; then \
	  echo "ERROR: Couldn't find $(myconfig) anywhere"; \
	  exit 1; \
	fi
